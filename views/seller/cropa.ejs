<table id="dtBasicExample" class="table table-striped table-bordered table-responsive-md" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th class="th-sm">Soil
      </th>
      <th class="th-sm">Temperature
      </th>
      <th class="th-sm">Humidity
      </th>
      <th class="th-sm">Ph
      </th>
      <th class="th-sm">Rainfall
      </th>
      <th class="th-sm">Price
      </th>
      <th class="th-sm">Crop
      </th>
    </tr>
  </thead>
  <tbody>
 
    <%for(var i=0;i<data.length;i++){ %>
        <tr>
          <%if(data[i].crop==val){%>
          <td style="background-color: aqua;"> <%=data[i].soil%></td>
          <td style="background-color: aqua;"><%=data[i].temperature%></td>
          <td style="background-color: aqua;"><%=data[i].humidity%></td>
          <td style="background-color: aqua;"><%=data[i].Ph%></td>
          <td style="background-color: aqua;"><%=data[i].rainfall%></td>
          <td style="background-color: aqua;"><%=data[i].crop%></td>
          <td style="background-color: aqua;"><%=data[i].price%></td>
          <%}%>
          <%if(data[i].crop!=val){ %>
          <td> <%=data[i].soil%></td>
          <td><%=data[i].temperature%></td>
          <td><%=data[i].humidity%></td>
          <td><%=data[i].Ph%></td>
          <td><%=data[i].rainfall%></td>
          <td><%=data[i].crop%></td>
          <td><%=data[i].price%></td>
          <%}%>
        </tr>
      <%}%>

  </tbody>
</table>
